{
  "name": "@sadorlovsky/eslint-config",
  "version": "0.13.0",
  "description": "Sad Orlovsky Codestyle - ESLint Shareable Config",
  "license": "MIT",
  "repository": "sadorlovsky/codestyle",
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "clinton": "clinton",
    "release": "npmpub",
    "rules": "eslint-find-rules -u",
    "check": "npm run test && npm run lint && npm run clinton",
    "prepush": "npm run check",
    "prepublish": "npm run check"
  },
  "files": [
    "index.js",
    "ava.js",
    "react.js"
  ],
  "keywords": [
    "sadorlovsky",
    "codestyle",
    "eslint",
    "shareable",
    "config"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "clinton": "^0.8.0",
    "eslint": "^3.4.0",
    "eslint-find-rules": "^1.13.1",
    "eslint-plugin-ava": "^3.0.0",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "husky": "^0.11.7",
    "is-plain-obj": "^1.1.0",
    "npmpub": "^3.1.0"
  },
  "peerDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.4.0",
    "eslint-plugin-ava": "^3.0.0",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0"
  },
  "clinton": {
    "rules": {
      "xo": "off",
      "ava": "error",
      "filename-case": "off"
    }
  }
}
