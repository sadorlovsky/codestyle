    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "is-plain-obj": "^2.1.0",
    "nyc": "^15.0.0",
    "react": "^16.12.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-ava": "^10.0.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3"
